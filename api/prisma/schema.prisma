datasource db {
    provider = "sqlite"
    url      = "file:../db/deezrupt.db"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id Int @id @default(autoincrement())
    userID String @unique
    accounts Account[]
}

model Account {
    id Int @id @default(autoincrement())
    accountID String @unique
    owner User @relation(fields: [ownerId], references: [userID])
    ownerId String
}